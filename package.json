  "scripts": {
    "build:js": "browserify -t reactify js-source/main.js > ./.tmp/javascripts/all.js",
    "build:css": "./node_modules/node-sass/bin/node-sass sass-source/ --include-path ./node_modules/ -o .tmp/stylesheets/",
    "build:sg": "npm run build:css & node_modules/sc5-styleguide/bin/styleguide --kss-source \"sass-source/*\" --style-source \".tmp/stylesheets/*\" --output source/styleguide --overviewPath \"README.md\" --appRoot \"/styleguide\"",
    "build:font": "cp -fr ./node_modules/bootstrap-sass/assets/fonts/ ./.tmp/fonts/",
    "build": "npm run build:js & npm run build:font & npm run build:css & npm run build:sg",
    "watch:js": "watch 'npm run build:js' js-source/ -du",
    "watch:css": "watch 'npm run build:css' sass-source/ -du",
    "watch:sg": "watch 'npm run build:sg' sass-source/ -du",
    "watch": "npm run watch:js & npm run watch:css & npm run watch:sg & npm run build:font",
    "test": "karma start"
  },
